{
  "name": "Oman Auto Mart",
  "short_name": "OmanAutoMart",
  "description": "Find and list cars for sale in Oman",
  "id": "oman-auto-mart-v1",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#000000",
  "lang": "en",
  "icons": [
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.omanautomart",
      "id": "com.omanautomart"
    }
  ],
  "prefer_related_applications": false,
  "scope_extensions": [
    {
      "origin": "https://omanautomart.com"
    }
  ],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "launch_handler": {
    "client_mode": "auto"
  },
  "display_override": ["window-controls-overlay"],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/open-car-file",
      "accept": {
        "application/json": [".car"]
      }
    }
  ],
  "handle_links": "preferred",
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "car",
          "accept": ["application/json", ".car"]
        }
      ]
    }
  },
  "protocol_handlers": [
    {
      "protocol": "web+car",
      "url": "/car?id=%s"
    }
  ]
}